<?xml version="1.0" encoding="utf-8" ?>
<monitor>
  <name>EventsServiceMonitor</name>
  <type>managed</type>
  <description>Sends windows events to ADQL</description>
  <monitor-configuration></monitor-configuration>
  <monitor-run-task>
    <!--  Edit execution-style as needed. -->
    <execution-style>periodic</execution-style>
    <name>Run</name>
    <type>executable</type>
    <task-arguments></task-arguments>
    <executable-task>
      <type>file</type>
      <file os-type="windows">start.bat</file>
    </executable-task>
  </monitor-run-task>
</monitor>
